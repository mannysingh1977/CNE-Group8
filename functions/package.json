{
  "name": "cne-group8-functions",
  "version": "1.0.0",
  "description": "Azure Functions for CNE Group 8 project",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "prestart": "npm run clean && npm run build",
    "start": "/opt/homebrew/bin/func start",
    "test": "echo \"No tests yet...\"",
    "deploy": "npm run build && func azure functionapp publish cne-group8-functions"
  },
  "dependencies": {
    "@azure/cosmos": "^4.2.0",
    "@azure/identity": "^4.7.0",
    "@azure/storage-blob": "^12.17.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.0",
    "jsonwebtoken": "^9.0.0",
    "uuid": "^9.0.1",
    "formidable": "^3.5.1"
  },
  "devDependencies": {
    "@azure/functions": "^3.0.0",
    "@types/bcryptjs": "^2.4.4",
    "@types/formidable": "^3.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.x",
    "@types/uuid": "^9.0.8",
    "azure-functions-core-tools": "^4.x",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.0"
  }
}
