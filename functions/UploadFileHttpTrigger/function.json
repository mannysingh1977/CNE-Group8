{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post"],
      "route": "upload"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "scriptFile": "../dist/UploadFileHttpTrigger/index.js",
  "extensions": {
    "http": {
      "routePrefix": "api",
      "maxRequestSize": 10485760
    }
  }
}
